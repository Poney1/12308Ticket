<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="25dp"
    android:gravity="center_vertical"
    android:orientation="horizontal">

    <TextView
        android:id="@+id/tv_ticket_fromdate"
        android:layout_width="0dp"
        android:layout_height="25dp"
        android:gravity="center"
        android:layout_weight="3"
        android:text="07-12"
        android:textColor="@color/c_333333"
        android:textSize="11sp" />

    <TextView
        android:id="@+id/tv_ticket_name"
        android:layout_width="0dp"
        android:layout_height="25dp"
        android:gravity="center"
        android:layout_weight="3"
        android:text="诸葛南"
        android:textColor="@color/c_333333"
        android:textSize="11sp" />

    <TextView
        android:id="@+id/tv_ticket_fromtime"
        android:layout_width="0dp"
        android:layout_height="25dp"
        android:gravity="center"
        android:layout_weight="4"
        android:text="08:05"
        android:textColor="@color/c_333333"
        android:textSize="11sp" />

    <TextView
        android:id="@+id/tv_ticket_fromstation"
        android:layout_width="0dp"
        android:layout_height="25dp"
        android:gravity="center"
        android:layout_weight="3"
        android:text="深圳北"
        android:textColor="@color/c_333333"
        android:textSize="11sp" />

    <TextView
        android:id="@+id/tv_ticket_totime"
        android:layout_width="0dp"
        android:layout_height="25dp"
        android:gravity="center"
        android:layout_weight="4"
        android:text="08:55"
        android:textColor="@color/c_333333"
        android:textSize="11sp" />

    <TextView
        android:id="@+id/tv_ticket_tostation"
        android:layout_width="0dp"
        android:layout_height="25dp"
        android:gravity="center"
        android:layout_weight="3"
        android:text="广州南"
        android:textColor="@color/c_333333"
        android:textSize="11sp" />

    <TextView
        android:id="@+id/tv_ticket_ticketstatus"
        android:layout_width="0dp"
        android:layout_height="25dp"
        android:gravity="center"
        android:layout_weight="4"
        android:text="已出票"
        android:textColor="@color/c_333333"
        android:textSize="11sp" />

</LinearLayout>